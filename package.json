    "backstopjs": "^6.2.2",
    "chalk": "^5.3.0",
    "js-yaml": "^4.1.0",
    "ncp": "^2.0.0",
    "slash": "^5.1.0",
    "tsx": "^4.19.2"
  },
  "devDependencies": {
    "@types/backstopjs": "^6.1.2",
    "@types/js-yaml": "^4.0.8",
    "@types/node": "^20.8.10",
    "ts-node": "^10.9.2",
    "typescript": "^5.2.2"
  },
  "peerDependencies": {
    "tsx": "^4.19.2"
