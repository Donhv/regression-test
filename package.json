{
  "name": "regressify",
  "version": "1.0.32",
  "description": "Visual regression tests support",
  "main": "src/index.ts",
  "type": "module",
  "bin": {
    "regressify": "./cli/index.js"
  },
  "scripts": {
    "build": "tsc --project tsconfig.json",
